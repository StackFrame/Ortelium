default: all

all:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs $@.svg

classes:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs $@.svg

clean:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs $@.svg

distclean:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs $@.svg

docs:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs $@.svg

clean_docs:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs $@.svg

jar:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs $@.svg

help:
	cd ${OPENMAP_HOME}/src/svg && ${OPENMAP_HOME}/bin/ant -emacs -projecthelp



